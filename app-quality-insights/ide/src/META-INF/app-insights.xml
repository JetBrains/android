<idea-plugin>
  <depends>org.intellij.intelliLang</depends>
  <extensions defaultExtensionNs="com.intellij">
    <library.toolWindow id="App Quality Insights"
                librarySearchClass="com.android.tools.idea.sdk.AndroidSdkLibrarySearcher"
                anchor="bottom"
                icon="StudioIcons.Shell.ToolWindows.APP_QUALITY_INSIGHTS"
                factoryClass="com.android.tools.idea.insights.ui.AppInsightsToolWindowFactory"
                doNotActivateOnStart="false"/>
    <projectService
        serviceInterface="com.android.tools.idea.insights.analytics.AppInsightsTracker"
        serviceImplementation="com.android.tools.idea.insights.analytics.AppInsightsTrackerImpl" />
    <applicationService
        serviceInterface="com.android.tools.idea.insights.AppInsightsService"
        serviceImplementation="com.android.tools.idea.insights.AppInsightsServiceImpl" />
  </extensions>
  <extensionPoints>
    <extensionPoint qualifiedName="com.android.tools.idea.insights.ui.appInsightsTabProvider"
                    interface="com.android.tools.idea.insights.ui.AppInsightsTabProvider"/>
  </extensionPoints>
</idea-plugin>